{
  "name": "tree-sitter-plantuml",
  "version": "2.1.0",
  "description": "PlantUML grammar for tree-sitter - Two-Pass Architecture with Browser Support",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./wasm": {
      "types": "./dist/wasm.d.ts",
      "import": "./dist/wasm.mjs",
      "require": "./dist/wasm.cjs"
    },
    "./normalizer": {
      "types": "./dist/normalizer.d.ts",
      "import": "./dist/normalizer.mjs",
      "require": "./dist/normalizer.cjs"
    },
    "./tree-sitter-plantuml.wasm": "./tree-sitter-plantuml.wasm",
    "./package.json": "./package.json"
  },
  "keywords": [
    "tree-sitter",
    "plantuml",
    "parser",
    "uml",
    "diagram",
    "normalizer",
    "activity-diagram",
    "wasm",
    "browser",
    "webassembly"
  ],
  "author": "PlantUML Parser Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SaeedNMosleh/PlantUML-Parser"
  },
  "scripts": {
    "build:js": "tsup",
    "build:native": "node-gyp rebuild",
    "build:wasm": "node scripts/build-wasm.js",
    "build:all": "npm run generate && npm run build:native && npm run build:wasm && npm run build:js",
    "test": "npm run build:all && npm run test:normalizer && npm run test:parser",
    "test:normalizer": "jest test/normalizer --coverage --collectCoverageFrom=dist/normalizer.cjs",
    "test:parser": "node scripts/ensure-tree-sitter-cli.js && tree-sitter test",
    "test:integration": "jest test/integration --coverage --collectCoverageFrom=dist/index.cjs --collectCoverageFrom=dist/normalizer.cjs",
    "test:all": "npm run validate && npm test && npm run test:integration",
    "test:ci": "npm run build:all && npm run test:normalizer && npm run test:parser && npm run test:integration",
    "test:100": "npm run test:all && echo 'âœ… 100% Success Rate Achieved!'",
    "generate": "node scripts/ensure-tree-sitter-cli.js && tree-sitter generate",
    "parse": "node scripts/ensure-tree-sitter-cli.js && tree-sitter parse",
    "clean": "rimraf build coverage dist prebuilds *.obj *.backup *.wasm",
    "rebuild": "npm run clean && npm run build:all",
    "prebuild": "node scripts/prebuild.js",
    "install": "node-gyp-build",
    "validate": "node scripts/validate-plantuml.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:validate": "npm run docker:up && npm run validate",
    "prepublishOnly": "npm run build:all && npm run prebuild && npm run test:ci"
  },
  "dependencies": {
    "node-addon-api": "^7.0.0",
    "node-gyp-build": "^4.6.0",
    "tree-sitter": "^0.20.6"
  },
  "peerDependencies": {
    "web-tree-sitter": "^0.20.0 || ^0.21.0 || ^0.22.0"
  },
  "peerDependenciesMeta": {
    "web-tree-sitter": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "tree-sitter-cli": "^0.20.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^22.0.0",
    "c8": "^9.0.0",
    "jest": "^29.7.0",
    "node-gyp": "^10.0.0",
    "prebuildify": "^6.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.0.0",
    "typescript": "^5.6.3",
    "web-tree-sitter": "^0.22.6"
  },
  "tree-sitter": [
    {
      "scope": "source.plantuml",
      "file-types": [
        "puml",
        "plantuml",
        "pu",
        "uml"
      ],
      "injection-regex": "plantuml"
    }
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "queries",
    "bindings/node/binding.cc",
    "src/parser.c",
    "src/node-types.json",
    "src/tree_sitter",
    "dist",
    "prebuilds",
    "*.wasm"
  ]
}
